# P1 single-machine defaults
DEPLOY_PROFILE=P1
RUN_MODE=api
DATABASE_URL=sqlite:///./metadata.sqlite
VECTOR_INDEX_BACKEND=memory
ORIGINALS_PATH=/data/originals
DERIVED_PATH=/data/derived
ENABLE_INLINE_WORKER=true
WORKER_CONCURRENCY=2
WORKER_POLL_INTERVAL=0.5
MAX_TASK_RETRIES=3
RETRY_BACKOFF_BASE_SECONDS=2.0
RETRY_BACKOFF_CAP_SECONDS=300
EMBED_MODEL_IMAGE=stub-clip
EMBED_MODEL_TEXT=stub-clip
